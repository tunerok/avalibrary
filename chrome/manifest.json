   {

      "manifest_version": 2,

      "name": "Avaproxy",
      "description": "Управление настройками прокси у браузера",
      "version": "0.2",
      "options_page": "options.html",
      "background": {
      "scripts":["background.js"]
      },
      "browser_action": {
            "default_icon": "icon.png"
        },
    "permissions": [
        
        "storage",
        "proxy"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
    }